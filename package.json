{
  "name": "expo-hamro-image-picker",
  "repository": {
    "type": "git",
    "url": "https://github.com/snlbaral/expo-hamro-image-picker.git"
  },
  "version": "0.0.1",
  "description": "A Image Picker/Selector library for react native expo",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "/dist"
  ],
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "expo-asset": "^8.6.2",
    "expo-file-system": "^15.1.1",
    "expo-gl": "^12.0.1",
    "expo-gl-cpp": "^11.4.0",
    "expo-image-editor": "^1.7.1",
    "expo-image-manipulator": "^11.0.0",
    "expo-image-picker": "^14.0.2",
    "expo-media-library": "^15.0.0",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-modalbox": "^2.0.2",
    "recoil": "^0.7.6"
  },
  "devDependencies": {
    "typescript": "^4.9.4",
    "react": "^16.9.0",
    "react-native": "^0.69.1",
    "@types/react-native-modalbox": "^1.4.10",
    "@tsconfig/react-native": "^2.0.2",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25"
  },
  "scripts": {
    "build": "prettier --write src/ && npm run build:esm && npm run build:cjs && copyfiles src/assets/*.png dist && copyfiles src/assets/*.png dist/cjs",
